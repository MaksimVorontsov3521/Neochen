<Window x:Class="Points.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Points"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="MainGrid">
        <Button x:Name="BigFallsend" Content="BigSand" HorizontalAlignment="Left" Height="42" Margin="10,10,0,0" VerticalAlignment="Top" Width="106" Click="BigFallsend_Click"/>
        <Button x:Name="SmallSand" Content="SmallSand" HorizontalAlignment="Left" Height="42" Margin="10,70,0,0" VerticalAlignment="Top" Width="106" Click="Button_Click" IsCancel="True"/>
        <Button x:Name="Jump_but" Content="Jump" HorizontalAlignment="Left" Margin="161,10,0,0" VerticalAlignment="Top" Width="106" Height="42" Click="Jump_Click"/>
        <Button x:Name="Cube_button" Content="Cube" HorizontalAlignment="Left" Height="42" Margin="161,70,0,0" VerticalAlignment="Top" Width="106" Click="Cube_button_Click"/>
        <Button x:Name="Snake" Content="Snake" HorizontalAlignment="Left" Height="42" Margin="316,10,0,0" VerticalAlignment="Top" Width="106" Click="Snake_Click"/>
    </Grid>
</Window>
